{
  "name": "@centcom/ui",
  "version": "0.0.5",
  "description": "Front-end for the CentCom SS13 Management System",
  "main": "src/index.js",
  "scripts": {
    "test": "npx mocha",
    "start:local": "webpack-dev-server --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.prod.js",
    "build:local": "exit 0",
    "deploy": "node scripts/deploy",
    "npm:publish": "npm publish --access public"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": true
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "antd": "^3.18.1",
    "connected-react-router": "^6.4.0",
    "fuse.js": "^3.4.4",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.9.0",
    "node-rsa": "^1.0.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-html-parser": "^2.0.2",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "readable-stream": "^2.3.6",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "striptags": "^3.1.1",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "xhr": "^2.5.0"
  },
  "devDependencies": {}
}
