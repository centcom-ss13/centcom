    "config": "^3.1.0",
    "generate-password": "^1.4.1",
    "hapi": "^18.1.0",
    "localstorage-memory": "^1.0.3",
    "mysql": "^2.17.1",
    "node-rsa": "^1.0.5",
    "request": "^2.88.0",
    "require-dir": "^1.2.0",
    "sqlstring": "^2.3.1",
    "squel": "^5.12.2",
    "validator": "^10.11.0"
  },
