{
  "name": "@centcom/centcom",
  "version": "0.0.1",
  "description": "Monorepo for the CentCom SS13 Management System",
  "main": "src/index.js",
  "scripts": {
    "bootstrap": "npx lerna bootstrap",
    "test": "npx lerna run test",
    "start": "npx lerna run start --parallel --stream",
    "build:local": "npx lerna run build:local",
    "build": "npx lerna run build",
    "deploy": "npx lerna run build:local",
    "npm:publish": "npm publish --access public",
    "prepare": "npx -p \"npm-run-all\" run-s globals bootstrap build",
    "globals": "npm i lerna npm-run-all -g"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": true
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "aws-sdk": "^2.456.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-wildcard": "^5.0.0",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "chardet": "^0.7.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "depcheck": "^0.7.2",
    "lerna": "^3.18.3",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "mime-types": "^2.1.24",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "node-sass": "^4.12.0",
    "proxyquire": "^2.1.0",
    "react-hot-loader": "^4.8.4",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-encoding-plugin": "^0.3.1",
    "webpack-merge": "^4.2.1"
  }
}
